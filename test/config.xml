<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="12" id="com.example.test" ios-CFBundleVersion="12" version="1.1.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Test</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <platform name="ios">
        <preference name="Orientation" value="all" />
    </platform>
    <platform name="android">
        <access origin="*" />
        <allow-navigation href="*" />
    </platform>
    <plugin name="cordova-plugin-appversion" spec="^1.0.0" />
    <plugin name="cordova-plugin-device" spec="^1.0.1" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.2.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="^1.0.1" />
    <plugin name="cordova-plugin-network-information" spec="^1.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.0.0" />
    <engine name="android" spec="^4.1.0" />
    <engine name="ios" spec="^3.9.0" />
</widget>
